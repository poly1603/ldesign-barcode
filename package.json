{
  "name": "@ldesign/barcode",
  "version": "0.1.0",
  "description": "条形码组件 - 多种格式、扫描识别、批量生成",
  "keywords": ["ldesign", "barcode", "scanner", "qrcode", "ean", "upc", "code128"],
  "author": "LDesign Team",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.cjs",
      "types": "./es/index.d.ts"
    },
    "./vue": {
      "import": "./es/adapters/vue/index.js",
      "types": "./es/adapters/vue/index.d.ts"
    },
    "./react": {
      "import": "./es/adapters/react/index.js",
      "types": "./es/adapters/react/index.d.ts"
    }
  },
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts"
  },
  "dependencies": {
    "@ldesign/shared": "workspace:*",
    "@ericblade/quagga2": "^1.8.4"
  },
  "peerDependencies": {
    "vue": ">=3.0.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/react": "^18.0.0",
    "typescript": "^5.7.3",
    "vue": "^3.0.0"
  }
}

