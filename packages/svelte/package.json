{
  "name": "@ldesign/barcode-svelte",
  "version": "0.1.0",
  "description": "条形码 Svelte 组件和 stores",
  "keywords": [
    "ldesign",
    "barcode",
    "svelte",
    "component",
    "store"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "svelte": "./es/index.js",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./components": {
      "types": "./es/components/index.d.ts",
      "svelte": "./es/components/index.js",
      "import": "./es/components/index.js",
      "require": "./lib/components/index.cjs"
    },
    "./stores": {
      "types": "./es/stores/index.d.ts",
      "import": "./es/stores/index.js",
      "require": "./lib/stores/index.cjs"
    }
  },
  "svelte": "./es/index.js",
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts",
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist es lib coverage"
  },
  "dependencies": {
    "@ldesign/barcode-core": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@ldesign/builder": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^4.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.18.0",
    "happy-dom": "^12.10.0",
    "rimraf": "^5.0.5",
    "svelte": "^4.2.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "libraries/barcode/packages/svelte"
  },
  "publishConfig": {
    "access": "public"
  }
}
